{
    "name": "oxidmod/json-rpc-server",
    "description": "Simple JSON-RPC server for your application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Melko",
            "email": "oxidmod@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oxidmod\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oxidmod\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
